super + w
    firefox

super + u: {k,m,s,l,r,c,u}
	{dmenukblayout, dmenumonitor, dmenusys, dmenuopgg, dmenuraco, dmenuconfig, dmenuopen}

XF86AudioRaiseVolume
	/usr/bin/pactl set-sink-volume @DEFAULT_SINK +5%

XF86AudioLowerVolume
	/usr/bin/pactl set-sink-volume @DEFAULT_SINK -5%

XF86AudioMute
	/usr/bin/pactl set-sink-mute @DEFAULT_SINK -5%

Print
	maim-ss

shift + Print
	maim-ss

shift + alt + Print
    maim-wiki

XF86MonBrightnessUp
	brightnessctl set +10%

XF86MonBrightnessDown
	brightnessctl set 10%

super + n
	redshift -O 5000

super + shift + n
	redshift -x
