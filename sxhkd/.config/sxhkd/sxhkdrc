super + p
	dmenu_run

super + u: {k,m,s}
	{dmenukblayout, dmenumonitor, dmenusys}

XF86AudioRaiseVolume
	/usr/bin/pactl set-sink-volume @DEFAULT_SINK +5%

XF86AudioLowerVolume
	/usr/bin/pactl set-sink-volume @DEFAULT_SINK -5%

XF86AudioMute
	/usr/bin/pactl set-sink-mute @DEFAULT_SINK -5%

Print
	maim-ss

XF86MonBrightnessUp
	brightnessctl set +10%

XF86MonBrightnessDown
	brightnessctl set 10%

super + n
	redshift -O 5000

super + shift + n
	redshift -x
